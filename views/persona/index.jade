doctype html

html(lang='en')
	
head
	title SACOE
	meta(charset='utf-8')
	meta(name="viewport" content="width=device-width, initial-scale=1.0")
	link(rel="stylesheet" href='../css/materialize.css')
	link(rel="stylesheet" href='../css/main.css')
	link(rel="stylesheet" href='../css/icon.css')
	link(rel="stylesheet" href='../css/fuente.css')
	script(type="text/javascript" src="../js/jquery-2.1.1.min.js")
	script(type="text/javascript" src="../js/materialize.min.js")
	
body(class="Oswald")

	header

		nav(class="indigo top-nav")

			div(class="nav-wrapper")

				ul
					li(class="hide-on-large-only")
						a(class="sideNav" data-activates="slide-out")
							i(class="material-icons left") menu

				a(href="../inicio" class="brand-logo Oswald margen-dere") SACOE

				ul(id="nav-mobile" class="right")

					li
						a(href="/salida")
							i(class="material-icons") exit_to_app

	main

		ul(id="slide-out" class="side-nav fixed")
			
			li(class="logo")
				img(class="responsive-img" src="../img/cac.jpg")

			li(class="search")

				div(class="row center")
			
				form(method="post" action="/buscar" class="col s12")

					div(class="row")

						div(class="col s1")

						div(class="input-field col s10 card")

							input(id="search" placeholder="Buscar..." class="deleteBorder" type="text" name="search")
				
						div(class="col s1")

			li(class="bold")
				a(href="../inicio" class="waves-effect waves-red") Inicio
					i(class="material-icons") home

			li(class="bold")
				a(href="./nuevo" class="waves-effect waves-red") Agregar
					i(class="material-icons") person_add
			li(class="bold")
				a(href="./" class="waves-effect waves-red") Ganados
					i(class="material-icons") people

			li(class="bold")
				a(href="./estadisticas" class="waves-effect waves-red") Estadisticas
					i(class="material-icons") equalizer



		div(class="row center")
			
			div(class="col s12")

				h4.center.margen-sup Heredades
					

				div.row.margen-sup
					
					div(class="col s4")

						a(href="/heredad/1" class="btn-floating hoverable waves-effect waves-light btn-large yellow tooltipped" data-position="top" data-delay="50" data-tooltip="#{dato.heredad1} Personas") H1
					div(class="col s4")

						a(href="/heredad/2" class="btn-floating hoverable waves-effect waves-light btn-large purple tooltipped" data-position="top" data-delay="50" data-tooltip="#{dato.heredad2} Personas") H2
					div(class="col s4")

						a(href="/heredad/3" class="btn-floating hoverable waves-effect waves-light btn-large pink tooltipped" data-position="top" data-delay="50" data-tooltip="#{dato.heredad3} Personas") H3
				
				div.row
					div(class="col s4")

						a(href="/heredad/4" class="btn-floating hoverable waves-effect waves-light btn-large indigo tooltipped" data-position="right" data-delay="50" data-tooltip="#{dato.heredad4} Personas") H4
					div(class="col s4")

						a(href="/heredad/5" class="btn-floating hoverable waves-effect waves-light btn-large white black-text tooltipped" data-position="bottom" data-delay="50" data-tooltip="#{dato.heredad5} Personas") H5
					div(class="col s4")

						a(href="/heredad/6" class="btn-floating hoverable waves-effect waves-light btn-large white black-text tooltipped" data-position="left" data-delay="50" data-tooltip="#{dato.heredad6} Personas") H6
				
				div.row
					div(class="col s4")

						a(href="/heredad/7" class="btn-floating hoverable waves-effect waves-light btn-large white black-text tooltipped" data-position="right" data-delay="50" data-tooltip="#{dato.heredad7} Personas") H7
					div(class="col s4")

						a(href="/heredad/8" class="btn-floating hoverable waves-effect waves-light btn-large white black-text tooltipped" data-position="bottom" data-delay="50" data-tooltip="#{dato.heredad8} Personas") H8
					div(class="col s4")

						a(href="/heredad/9" class="btn-floating hoverable waves-effect waves-light btn-large light-green accent-3 tooltipped" data-position="left" data-delay="50" data-tooltip="#{dato.heredad9} Personas") H9
				div.row
					div(class="col s4")

						a(href="/heredad/10" class="btn-floating hoverable waves-effect waves-light btn-large white black-text tooltipped" data-position="right" data-delay="50" data-tooltip="#{dato.heredad10} Personas") H10
					div(class="col s4")

						a(href="/heredad/11" class="btn-floating hoverable waves-effect waves-light btn-large white black-text tooltipped" data-position="bottom" data-delay="50" data-tooltip="#{dato.heredad11} Personas") H11
					div(class="col s4")

						a(href="/heredad/12" class="btn-floating hoverable waves-effect waves-light btn-large white black-text tooltipped" data-position="left" data-delay="50" data-tooltip="#{dato.heredad12} Personas") H12

				div.row
					div(class="col s4")
					div(class="col s4")
						a(href="/todos" class="btn-floating hoverable waves-effect waves-light btn-large red black-text tooltipped" data-position="bottom" data-delay="50" data-tooltip="#{dato.todos} Personas") T
					div(class="col s4")

	footer



												
	script(type="text/javascript" src="../js/componentsMaterialize.js")